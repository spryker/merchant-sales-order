{
  "name": "spryker/merchant-sales-order",
  "type": "library",
  "description": "MerchantSalesOrder module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/calculation": "^4.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-sales-order-extension": "^0.1.2",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^11.18.0",
    "spryker/sales-extension": "^1.1.0",
    "spryker/shipment-extension": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/oms": "*",
    "spryker/product-offer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\MerchantSalesOrder\\Helper\\": "tests/SprykerTest/Zed/MerchantSalesOrder/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
