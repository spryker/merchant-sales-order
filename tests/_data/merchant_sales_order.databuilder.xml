<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="MerchantOrder">
        <property name="merchantSalesOrderReference" dataBuilderRule="unique()->word"/>
        <property name="idSalesOrder" dataBuilderRule="randomNumber(3)"/>
        <property name="merchantReference" dataBuilderRule="unique()->word"/>
        <property name="totals" dataBuilderRule="Totals"/>
    </transfer>

</transfers>
